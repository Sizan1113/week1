#Bank Balance checker
Balance = 5000
withdraw = int(input("Enter amount to withdraw:"))
if withdraw < 1000:
    print("The amount must be greater or equal to 1000")
    if withdraw > Balance:
        print("Insufficient amoount")
   
    else:
        Balance = Balance - withdraw
        print(f"Remaining balance:",Balance)




amount = 5000
withdraw = int(input("Enter amount to withdraw:"))
if withdraw <= amount:
    print("Processing......")
    if withdraw % 100 == 0:
        amount = amount - withdraw
        print(f"remaining balance:",amount)
    else:
        print("Enter amount which is multiple of 100")
else:
    print("Insufficient Fund:")





